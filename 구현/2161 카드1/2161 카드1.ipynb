   "source": [
    "from collections import deque\n",
    "\n",
    "N=int(input())\n",
    "queue=deque([i for i in range(1,N+1)])\n",
    "\n",
    "while 1:\n",
    "    if len(queue)==1:\n",
    "        print(queue[0])\n",
    "        break\n",
    "    first=queue.popleft()\n",
    "    print(first, end=\" \")\n",
    "    second=queue.popleft()\n",
    "    queue.append(second)"
   ]
  }
