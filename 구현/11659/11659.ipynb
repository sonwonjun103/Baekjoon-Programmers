    "import sys\n",
    "input=sys.stdin.readline\n",
    "\n",
    "N,M=map(int, input().split())\n",
    "\n",
    "num=list(map(int, input().split()))\n",
    "result=[0]\n",
    "\n",
    "temp=0\n",
    "for i in num:\n",
    "    temp+=i\n",
    "    result.append(temp)\n",
    "\n",
    "for _ in range(M):\n",
    "    i,j=map(int, input().split())\n",
    "\n",
    "    print(result[j]-result[i-1])"
   ]
  }
