    "N,K=map(int, input().split())\n",
    "\n",
    "dp=[[0]*201 for i in range(0,201)]\n",
    "\n",
    "for i in range(201):\n",
    "    dp[1][i]=1\n",
    "    dp[2][i]=i+1\n",
    "    \n",
    "for i in range(2,201):\n",
    "    dp[i][1]=i\n",
    "    for j in range(2,201):\n",
    "        dp[i][j]=(dp[i-1][j]+dp[i][j-1])%1000000000\n",
    "        \n",
    "print(dp[K][N])"
   ]
