    "N=int(input())\n",
    "\n",
    "def check(N):\n",
    "    cnt=0\n",
    "    while 1:\n",
    "        if (N%5)==0:\n",
    "            a=N//5\n",
    "            cnt+=a\n",
    "            break\n",
    "        N-=3\n",
    "        cnt+=1\n",
    "        \n",
    "    if N<0:\n",
    "        return -1\n",
    "    else:\n",
    "        return cnt\n",
    "    \n",
    "print(check(N))"
   ]
