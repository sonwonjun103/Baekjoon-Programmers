   "source": [
    "N=int(input())        \n",
    "K=int(input())\n",
    "\n",
    "start=1\n",
    "end=K\n",
    "\n",
    "while start<=end:\n",
    "    mid=(start+end)//2\n",
    "    \n",
    "    temp=0\n",
    "    for i in range(1,N+1):\n",
    "        temp+=min(mid//i, N)\n",
    "        \n",
    "    if temp>=K:\n",
    "        answer=mid\n",
    "        end=mid-1\n",
    "    else:\n",
    "        start=mid+1\n",
    "print(answer)"
   ]
  }
 ],
